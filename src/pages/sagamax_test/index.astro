---
import '../../styles/style.css';
import '../../styles/print.css';
import { Image } from 'astro:assets';
import favicon from "../../assets/images/favicon.ico"
import icon_top from "../../assets/images/icon_top.png"
import icon_twitter from "../../assets/images/icon_twitter.png"
import icon_twitter_bot from "../../assets/images/icon_twitter_bot.png"
import icon_youtube from "../../assets/images/icon_youtube.png"
import icon_twitch from "../../assets/images/icon_twitch.webp"
---

<!DOCTYPE html>
<html lang="ja" prefix="og: http://ogp.me/ns#">
  <head>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <meta charset="UTF-8">
    <meta http-equiv="Pragma" content="no-cache">
    <meta http-equiv="cache-control" content="no-cache">
    <meta http-equiv="expires" content="0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="Content-Style-Type" content="text/css" />
    <meta http-equiv="Content-Script-Type" content="text/javascript" />
    <meta http-equiv="refresh" content="3;URL=https://taumax-github.github.io/sagamax/index.html"> リダイレクト
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- twitter投稿用の設定 -->
    <meta name="twitter:card" content="summary" /> <!--カード種類：summary か summary_large_image-->
    <meta name="twitter:site" content="@sagamax__" /> <!--ユーザー名-->
    <meta property="og:url" content="https://taumax-github.github.io/sagamax/index.html" /> <!--記事のURL-->
    <meta property="og:title" content="sagamaxのサガとレトロゲー" /> <!--記事のタイトル-->
    <meta property="og:description" content="sagamaxのサガとレトロゲートップページ" /> <!--記事の要約（ディスクリプション）-->
    <meta property="og:image" content="https://taumax-github.github.io/sagamax/images/icon_twitter_card.png" /> <!--画像のURL-->

    <link rel="shortcut icon"    type="image/x-icon" href={favicon}>
    <link rel="apple-touch-icon"                     href={favicon} sizes="180x180">
    <link rel="icon"             type="image/png"    href={favicon} sizes="192x192">
    <script src="../../js/openclose.js"></script>

    <title>sagamax site</title>

    <!-- Global site tag (gtag.js) - Google Analytics -->
    <!-- <script async src="https://www.googletagmanager.com/gtag/js?id=UA-156501005-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());
      gtag('config', 'UA-156501005-1');
    </script>
    <script data-ad-client="ca-pub-5924490903263360" async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script> -->
  </head>

<!--PC用（801px以上端末）で表示させるブロック-->
<header class="pc">
  <h1 class="logo">
    <a href="https://taumax-github.github.io/sagamax/">
      <Image src={icon_top} alt="TOP"><br>
      <font size="3px">TOP</font>
    </a>
  </h1>

  <!--PC用（801px以上端末）メニュー-->
  <ul class="icon">
    <li><a href="https://twitter.com/sagamax__" target="_blank"><Image src={icon_twitter} alt="Twitter"></a></li>
    <li><a href="https://twitter.com/sagamax_bot" target="_blank"><Image src={icon_twitter_bot} alt="Twitter"></a></li>
    <li><a href="https://www.youtube.com/channel/UCFFZpzVyg22-2sxXE95cR_Q" target="_blank"><Image src={icon_youtube} alt="youtube"></a></li>
    <li><a href="https://www.twitch.tv/sagamax" target="_blank"><Image src={icon_twitch} alt="twitch"></a></li>
  </ul>
  <nav>
  </nav>
</header>
<!--/.pc-->

<!--小さな端末用（768px以下端末）で表示させるブロック-->
<header class="sh">
  <!--小さな端末用（768px以下端末）メニュー-->
  <div id="menubar-s" text-align="center">
    <nav>
    </nav>
    <h1 class="logo">
      <a href="https://taumax-github.github.io/sagamax/">
        <Image src={icon_top} alt="TOP">
        <br>
        <font size="3px">TOP</font>
      </a>
    </h1>
    <ul class="icon">
      <li><a href="https://twitter.com/sagamax__" target="_blank"><Image src={icon_twitter} alt="Twitter"></a></li>
      <li><a href="https://twitter.com/sagamax_bot" target="_blank"><Image src={icon_twitter_bot} alt="Twitter"></a></li>
      <li><a href="https://www.youtube.com/channel/UCFFZpzVyg22-2sxXE95cR_Q" target="_blank"><Image src={icon_youtube} alt="youtube"></a></li>
      <li><a href="https://www.twitch.tv/sagamax" target="_blank"><Image src={icon_twitch} alt="twitch"></a></li>
    </ul>
  </div><!--/#menubar-s-->
</header><!--/.sh-->

<body class="defaultbody">
  <div id="container">
  <div id="contents">
  <div id="main">
    <span id="pagetop"></span>
    <section class="box">
      <!-- tweetボタン -->
      <a href="https://twitter.com/share?ref_src=twsrc%5Etfw" class="twitter-share-button" data-show-count="false">Tweet</a><script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script><br>

      <h2 class="title">ｓａｇａｍａｘのサガとレトロゲー</h2>
      <p>
        転送します。<br>
        <a href="https://taumax-github.github.io/sagamax/index.html">移動先はこちら</a>。
      </p>

      <!-- <br> -->
      <!-- Google AdSense -->
      <!-- <script async src="https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script> -->
      <!-- 横長広告 -->
      <!-- <ins class="adsbygoogle"
           style="display:block"
           data-ad-client="ca-pub-5924490903263360"
           data-ad-slot="6254596512"
           data-ad-format="auto"
           data-full-width-responsive="true"></ins>
      <script>
           (adsbygoogle = window.adsbygoogle || []).push({});
      </script> -->
